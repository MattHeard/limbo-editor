#!/usr/bin/env ruby

descriptions = {
  'new' => 'creates a new project',
  'list-tests' => 'lists test descriptions'
}
descriptions.default = 'Limbo EDitor'

subcommand = ARGV[0]
if subcommand != 'list-tests' || ARGV[1] == '--help'
  puts descriptions[subcommand]
else
  puts "first\nsecond\nthird"
end
